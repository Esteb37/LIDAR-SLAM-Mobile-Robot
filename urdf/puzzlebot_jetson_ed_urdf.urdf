        </inertial>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://puzzlebot_sim/models/MCR2_1000_13_Chassis.stl"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.57079632679 0 1.57079632679" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://puzzlebot_sim/models/MCR2_1000_13_Chassis.stl"/>
            </geometry>
        </visual>
    </link>

	<link name="link_wr">
